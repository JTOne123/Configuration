<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;net451</TargetFrameworks>
    <Authors>nelson</Authors>
    <Description>the configuration provider that use zookeeper</Description>
    <Copyright>Copyright 2017 (c) nelson. All rights reserved.</Copyright>
    <PackageTags>zookeeper configuration</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/yuniansheng/Configuration</RepositoryUrl>
    <PackageProjectUrl>https://github.com/yuniansheng/Configuration</PackageProjectUrl>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Crisp.Extensions.Configuration.Zookeeper.xml</DocumentationFile>
  </PropertyGroup>  

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="1.1.2" />
    <PackageReference Include="ZooKeeperNetEx" Version="3.4.9.4" />
  </ItemGroup>

</Project>
